{
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ $fileName -I $workspaceRoot/ac-library -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "python": "python -u"
    },
    "code-runner.runInTerminal": true,
    "editor.tabCompletion": "on",
    "editor.snippetSuggestions": "top",
    "editor.quickSuggestions": {
        "other": true,
        "comments": true,
        "strings": true
    },
}
